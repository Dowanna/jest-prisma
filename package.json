{
  "private": true,
  "name": "@quramy/jest-prisma-workspaces",
  "scripts": {
    "prepare": "husky install",
    "bootstrap": "npm run bootstrap --workspace examples/simple-prj && npm run bootstrap --workspace packages/jest-prisma",
    "migrate:test": "npm run migrate:test:ci --workspace examples/simple-prj",
    "test:ci": "npm run test:ci --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run prettier -- --check",
    "format": "npm run prettier -- --write",
    "prettier": "prettier ."
  },
  "keywords": [],
  "author": "Quramy",
  "license": "MIT",
  "devDependencies": {
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.7.4"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ]
}
